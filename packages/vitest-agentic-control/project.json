{
  "name": "vitest-agentic-control",
  "tags": ["lang:ts", "scope:testing"],
  "targets": {
    "build": {
      "command": "tsc",
      "options": { "cwd": "{projectRoot}" }
    },
    "lint": {
      "command": "biome lint src/",
      "options": { "cwd": "{projectRoot}" }
    },
    "typecheck": {
      "command": "tsc --noEmit",
      "options": { "cwd": "{projectRoot}" }
    }
  }
}
