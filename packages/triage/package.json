{
    "name": "@jbcom/agentic-triage",
    "version": "1.1.0",
    "description": "AI-powered GitHub issue triage, PR review, and sprint planning - Vercel AI SDK primitives",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "agentic-triage": "./dist/cli.js",
        "triage": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./schemas": {
            "types": "./dist/schemas/index.d.ts",
            "import": "./dist/schemas/index.js"
        },
        "./tools": {
            "types": "./dist/tools/index.d.ts",
            "import": "./dist/tools/index.js"
        },
        "./handlers": {
            "types": "./dist/handlers/index.d.ts",
            "import": "./dist/handlers/index.js"
        },
        "./reporters/vitest": {
            "types": "./dist/reporters/vitest.d.ts",
            "import": "./dist/reporters/vitest.js"
        },
        "./reporters/playwright": {
            "types": "./dist/reporters/playwright.d.ts",
            "import": "./dist/reporters/playwright.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "format": "biome format --write .",
        "format:check": "biome format .",
        "check": "biome check .",
        "check:fix": "biome check --write .",
        "docs": "typedoc"
    },
    "keywords": [
        "triage",
        "ai",
        "automation",
        "agentic",
        "ollama",
        "anthropic",
        "vercel-ai-sdk",
        "mcp",
        "playwright",
        "testing",
        "security",
        "codeql",
        "sarif",
        "github",
        "octokit",
        "cli",
        "sprint-planning",
        "release-management",
        "issue-triage",
        "code-review"
    ],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jbcom/agentic.git",
        "directory": "packages/triage"
    },
    "homepage": "https://github.com/jbcom/agentic/tree/main/packages/triage#readme",
    "bugs": {
        "url": "https://github.com/jbcom/agentic/issues"
    },
    "engines": {
        "node": ">=20"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "@actions/core": "^3.0.0",
        "@ai-sdk/anthropic": "^3.0.48",
        "@ai-sdk/google": "^3.0.33",
        "@ai-sdk/mcp": "^1.0.21",
        "@linear/sdk": "^76.0.0",
        "@modelcontextprotocol/sdk": "^1.27.1",
        "@playwright/mcp": "latest",
        "ai": "^6.0.103",
        "ai-sdk-ollama": "^3.8.0",
        "commander": "^14.0.3",
        "glob": "^13.0.6",
        "octokit": "^5.0.5",
        "ollama": "^0.6.3",
        "picocolors": "^1.1.1",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@types/node": "^25.3.2",
        "@vitest/coverage-v8": "^4.0.18",
        "rimraf": "^6.1.2",
        "tsup": "^8.5.0",
        "tsx": "^4.21.0",
        "typedoc": "^0.28.17",
        "typescript": "^5.9.3",
        "vitest": "^4.0.18"
    }
}
