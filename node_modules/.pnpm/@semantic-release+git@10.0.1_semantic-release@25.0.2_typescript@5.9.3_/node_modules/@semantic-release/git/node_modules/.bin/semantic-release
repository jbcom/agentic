#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/bin/node_modules:/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/node_modules:/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/bin/node_modules:/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/node_modules:/workspace/node_modules/.pnpm/semantic-release@25.0.2_typescript@5.9.3/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/bin/semantic-release.js" "$@"
else
  exec node  "$basedir/../../../../../../semantic-release@25.0.2_typescript@5.9.3/node_modules/semantic-release/bin/semantic-release.js" "$@"
fi
